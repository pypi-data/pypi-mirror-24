{
    "service": {
        "groups": ["master"],
        "port": 6379,
	"apply_on": "redis",
	
	"check": {
	    
	    "ok_output": "'OK: Redis is available'",
	    
	    "critical_if": "{collector.redis.available} == False",
	    "critical_output": "'CRITICAL: Redis is down!'",
	    
	    "interval": "10s",
	    
	    "handlers": ["default"]
	}
    }
}

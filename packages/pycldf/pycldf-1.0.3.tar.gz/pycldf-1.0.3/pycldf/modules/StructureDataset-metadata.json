{
    "@context": "http://www.w3.org/ns/csvw",
    "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#StructureDataset",
    "dialect": {
        "commentPrefix": null
    },
    "tables": [
        {
            "url": "values.csv",
            "dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#ValueTable",
            "tableSchema": {
                "columns": [
                    {
                        "name": "ID",
                        "propertyUrl": "http://purl.org/dc/terms/identifier",
                        "required": true,
                        "datatype": "string"
                    },
                    {
                        "name": "Language_ID",
                        "propertyUrl": "http://linguistics-ontology.org/gold/2010/Language",
                        "required": true,
                        "datatype": "string"
                    },
                    {
                        "name": "Parameter_ID",
                        "titles": "Feature_ID",
                        "propertyUrl": "http://www.w3.org/2004/02/skos/core#Concept",
                        "required": true,
                        "datatype": "string"
                    },
                    {
                        "name": "Value",
                        "propertyUrl": "http://purl.org/linguistics/gold/dataStructureRelation",
                        "required": true,
                        "datatype": "string"
                    },
                    {
                        "name": "Comment",
                        "propertyUrl": "http://purl.org/dc/terms/description",
                        "datatype": "string"
                    },
                    {
                        "name": "Source",
                        "separator": ";",
                        "propertyUrl": "http://purl.org/dc/terms/source",
                        "datatype": "string"
                    }
                ],
                "primaryKey": "ID"
            }
        }
    ]
}

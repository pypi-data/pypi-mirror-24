{
	"url": "languages.csv",
	"dc:conformsTo": "http://cldf.clld.org/v1.0/terms.rdf#LanguageTable",
	"tableSchema": {
		"columns": [
			{
				"name": "ID",
				"propertyUrl": "http://purl.org/dc/terms/identifier",
				"datatype": "string"
			},
			{
				"name": "Name",
				"datatype": "string"
			},
			{
				"name": "Latitude",
				"propertyUrl": "http://www.w3.org/2003/01/geo/wgs84_pos#lat",
				"datatype": {
					"base": "decimal",
					"minimum": -90,
					"maximum": 90
				}
			},
			{
				"name": "Longitude",
				"propertyUrl": "http://www.w3.org/2003/01/geo/wgs84_pos#long",
				"datatype": {
					"base": "decimal",
					"minimum": -180,
					"maximum": 180
				}
			}
		],
		"primaryKey": "ID"
	}
}
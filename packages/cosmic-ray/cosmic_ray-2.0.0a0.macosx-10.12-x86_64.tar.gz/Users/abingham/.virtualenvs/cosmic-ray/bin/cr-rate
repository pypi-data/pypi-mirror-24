#!/Users/abingham/.virtualenvs/cosmic-ray/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'cosmic-ray==2.0.0a0','console_scripts','cr-rate'
__requires__ = 'cosmic-ray==2.0.0a0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cosmic-ray==2.0.0a0', 'console_scripts', 'cr-rate')()
    )

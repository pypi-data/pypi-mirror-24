<?xml version="1.0" encoding="UTF-8"?><xs:schema targetNamespace="http://nuu.application.i2/"	elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema"	xmlns:tns="http://nuu.application.i2/">	<xs:include schemaLocation="./domains.xsd" />	<xs:element name="interfaceNuu">		<xs:complexType>			<xs:sequence>				<xs:element name="entete" type="tns:t_entete" />				<xs:element name="operation" type="tns:t_operation" />				<xs:element name="listeDemandes" type="tns:t_listeDemandes"					minOccurs="0" />				<xs:element name="listeAnomalies" type="tns:t_listeAnomalies"					minOccurs="0" />			</xs:sequence>			<xs:attribute name="versionInterface" type="tns:t_version"				use="required" fixed="02.00" />		</xs:complexType>	</xs:element>	<!-- Définition de l'entête -->	<xs:complexType name="t_entete">		<xs:sequence>			<xs:element name="typeFichier" type="tns:t_code4" />			<xs:element name="dateFichier" type="xs:dateTime" />			<xs:element name="codeGuichet" type="tns:t_numGuichet" />		</xs:sequence>	</xs:complexType>	<!-- Définition de la section opération -->	<xs:complexType name="t_operation">		<xs:sequence>			<xs:element name="demandeMulticritere" type="tns:t_demandeMulticritere" />		</xs:sequence>	</xs:complexType>		<!-- Définition de la liste des types d'événements -->	<xs:complexType name="t_listeDemandes">		<xs:sequence>			<xs:element name="numUnique" type="tns:t_numUnique"				maxOccurs="unbounded" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="t_demandeMulticritere">		<xs:sequence>			<xs:element name="numUnique" type="tns:t_numUnique" minOccurs="0" />			<xs:element name="etatDemande" type="tns:t_code1" minOccurs="0" />			<xs:element name="dateDepotDebut" type="xs:date" minOccurs="0"  />			<xs:element name="dateDepotFin" type="xs:date" minOccurs="0"  />			<xs:element name="dateRenouvellementDebut" type="xs:date" minOccurs="0"  />			<xs:element name="dateRenouvellementFin" type="xs:date" minOccurs="0"  />			<xs:element name="dateRadiationDebut" type="xs:date" minOccurs="0"  />			<xs:element name="dateRadiationFin" type="xs:date" minOccurs="0"  />			<xs:element name="motifDemande" type="tns:t_string6" minOccurs="0" />			<xs:element name="motifRadiation" type="tns:t_string6" minOccurs="0" />			<xs:element name="communeSouhaitee" type="tns:t_code5" minOccurs="0" />			<xs:element name="communeAttribution" type="tns:t_code5" minOccurs="0" />			<xs:element name="estDelaiAnormalementLong" type="xs:boolean" minOccurs="0" />			<xs:element name="estMultiplesAttributionsSousConditions" type="xs:boolean" minOccurs="0" />			<xs:element name="association" type="tns:t_association" minOccurs="0" />			<xs:element name="personneRecherchee" type="tns:t_personneRecherchee" minOccurs="0" />			<xs:element name="piecesJustificatives" type="tns:t_piecesJustificatives" minOccurs="0" />		</xs:sequence>	</xs:complexType>	<!-- Définition d'une association -->	<xs:complexType name="t_association">		<xs:sequence>			<xs:element name="nom" type="tns:t_string36" />		</xs:sequence>	</xs:complexType>	<!-- Définition d'une personne recherché -->	<xs:complexType name="t_personneRecherchee">		<xs:sequence>			<xs:element name="nom" type="tns:t_string36" minOccurs="0" />			<xs:element name="prenom" type="tns:t_string36" minOccurs="0"/>			<xs:element name="dateNaissance" type="xs:date" minOccurs="0" />		</xs:sequence>	</xs:complexType>	<!-- Définition de la liste des codes de pièces justificatives -->	<xs:complexType name="t_piecesJustificatives">		<xs:sequence>			<xs:element name="codeCategorie" type="tns:t_code10"				minOccurs="1" maxOccurs="unbounded" />		</xs:sequence>	</xs:complexType>	<!-- Définition de la liste des anomalies -->	<xs:complexType name="t_listeAnomalies">		<xs:sequence>			<xs:element name="anomalie" type="tns:t_anomalie"				minOccurs="1" maxOccurs="unbounded" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="t_anomalie">		<xs:sequence>			<xs:element name="message" type="tns:t_string512" />		</xs:sequence>		<xs:attribute name="code" type="tns:t_string10" use="required" />	</xs:complexType></xs:schema>
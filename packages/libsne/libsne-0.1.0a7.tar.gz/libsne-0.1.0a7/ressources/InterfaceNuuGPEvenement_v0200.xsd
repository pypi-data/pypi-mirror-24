<?xml version="1.0" encoding="UTF-8"?><xs:schema targetNamespace="http://nuu.application.i2/"	elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema"	xmlns:tns="http://nuu.application.i2/">	<xs:include schemaLocation="./domains.xsd" />	<xs:element name="interfaceNuu">		<xs:complexType>			<xs:sequence>				<xs:element name="entete" type="tns:t_entete" />				<xs:element name="operation" type="tns:t_operation" />				<xs:element name="listeEvenements" type="tns:t_listeEvenements"					minOccurs="0" />				<xs:element name="listeAnomalies" type="tns:t_listeAnomalies"					minOccurs="0" />			</xs:sequence>			<xs:attribute name="versionInterface" type="tns:t_version"				use="required" fixed="02.00" />		</xs:complexType>	</xs:element>	<!-- Définition de l'entête -->	<xs:complexType name="t_entete">		<xs:sequence>			<xs:element name="typeFichier" type="tns:t_code3" />			<xs:element name="dateFichier" type="xs:dateTime" />			<xs:element name="codeGuichet" type="tns:t_numGuichet" />		</xs:sequence>	</xs:complexType>	<!-- Définition de la section opération -->	<xs:complexType name="t_operation">		<xs:sequence>			<xs:element name="numUnique" type="tns:t_numUnique" />		</xs:sequence>	</xs:complexType>		<!-- Définition de la liste des types d'événements -->	<xs:complexType name="t_listeEvenements">		<xs:sequence>			<xs:element name="evenement" type="tns:t_evenement"				maxOccurs="unbounded" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="t_evenement">		<xs:sequence>			<xs:element name="idEvenement" type="tns:t_positiveLong" minOccurs="0" />			<xs:element name="codeTypeEvenement" type="tns:t_code10" minOccurs="0"  />			<xs:element name="libelleTypeEvenement" type="tns:t_string100" minOccurs="0"  />			<xs:element name="auteurCreation" type="tns:t_string255" minOccurs="0" />			<xs:element name="dateCreation" type="xs:dateTime" minOccurs="0" />			<xs:element name="auteurModification" type="tns:t_string255" minOccurs="0" />			<xs:element name="dateModification" type="xs:dateTime" minOccurs="0" />			<xs:element name="natureModification" type="tns:t_string6" minOccurs="0" />			<xs:element name="dateSurvenance" type="xs:date" minOccurs="0" />			<xs:element name="commentaire" type="tns:t_string500" minOccurs="0" />			<xs:element name="reglementaire" type="xs:boolean" minOccurs="0" />			<xs:element name="listeCaracteristiquesSpecifiques" type="tns:t_listeCaracteristiquesSpecifiques"				minOccurs="0" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="t_listeCaracteristiquesSpecifiques">		<xs:sequence>			<xs:element name="caracteristiqueSpecifique" type="tns:t_caracteristiqueSpecifique"				maxOccurs="unbounded" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="t_caracteristiqueSpecifique">		<xs:sequence>			<xs:element name="code" type="tns:t_code10" />			<xs:element name="libelle" type="tns:t_string100" minOccurs="0"/>			<xs:element name="texteDescriptif" type="tns:t_string500" minOccurs="0"/>			<xs:element name="valeurCaracteristiqueSpecifique" type="tns:t_string100" minOccurs="0" />		</xs:sequence>	</xs:complexType>	<!-- Définition de la liste des anomalies -->	<xs:complexType name="t_listeAnomalies">		<xs:sequence>			<xs:element name="anomalie" type="tns:t_anomalie"				minOccurs="1" maxOccurs="unbounded" />		</xs:sequence>	</xs:complexType>	<xs:complexType name="t_anomalie">		<xs:sequence>			<xs:element name="message" type="tns:t_string512" />		</xs:sequence>		<xs:attribute name="code" type="tns:t_string10" use="required" />	</xs:complexType></xs:schema>
<form class="form-horizontal">
    <h3>Select upload dialog file types</h3>
    <div class="control-group">
        <div class="controls">
            <label class="checkbox">
                <input type="checkbox" data-bind="checked: type.stl" id="settings-stl"> stl
            </label>
        </div>
        <div class="controls">
            <label class="checkbox">
                <input type="checkbox" data-bind="checked: type.gcode" id="settings-gcode"> gcode
            </label>
        </div>
        <div class="controls">
            <label class="checkbox">
                <input type="checkbox" data-bind="checked: type.gco" id="settings-gco"> gco
            </label>
        </div>
        <div class="controls">
            <label class="checkbox">
                <input type="checkbox" data-bind="checked: type.g" id="settings-g"> g
            </label>
        </div>
    </div>
</form>

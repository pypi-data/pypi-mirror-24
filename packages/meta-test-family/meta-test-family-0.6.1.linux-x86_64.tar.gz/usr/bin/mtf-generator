#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'meta-test-family==0.6.1','console_scripts','mtf-generator'
__requires__ = 'meta-test-family==0.6.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('meta-test-family==0.6.1', 'console_scripts', 'mtf-generator')()
    )

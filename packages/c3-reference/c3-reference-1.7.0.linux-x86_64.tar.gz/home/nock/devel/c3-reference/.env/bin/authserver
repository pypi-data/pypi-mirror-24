#!/home/nock/devel/c3-reference/.env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'c3-reference==1.7.0','console_scripts','authserver'
__requires__ = 'c3-reference==1.7.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('c3-reference==1.7.0', 'console_scripts', 'authserver')()
    )

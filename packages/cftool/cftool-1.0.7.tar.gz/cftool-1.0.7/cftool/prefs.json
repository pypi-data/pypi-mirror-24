{
  "c++": {
    "compile": "g++ -std=c++11 {args} {file} -o main",
    "run": "./main",
    "extensions": [
      ".cpp",
      ".cc",
      ".cxx"
    ]
  },
  "c++14": {
    "compile": "g++ -std=c++14 {args} {file} -o main",
    "run": "./main",
    "extensions": [
      ".cpp",
      ".cc",
      ".cxx"
    ]
  },
  "c": {
    "compile": "gcc {args} {file} -o main",
    "run": "./main",
    "extensions": [
      ".c"
    ]
  },
  "python": {
    "compile": "pass",
    "run": "python {file}",
    "extensions": [
      ".py"
    ]
  },
  "python3": {
    "compile": "pass",
    "run": "python3 {file}",
    "extensions": [
      ".py"
    ]
  },
  "ruby:": {
    "compile": "pass",
    "run": "rb {file}",
    "extensions": [
      ".rb"
    ]
  }
}

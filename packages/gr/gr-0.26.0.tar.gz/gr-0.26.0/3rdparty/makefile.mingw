   MAKE = mingw32-make -f makefile.mingw

default:
	@$(MAKE) -C jbig2dec
	@$(MAKE) -C jpeg
	@$(MAKE) -C openjpeg
	@$(MAKE) -C libpng16
	@$(MAKE) -C zlib
	@$(MAKE) -C qhull
	
clean:
	@$(MAKE) -C jbig2dec clean
	@$(MAKE) -C jpeg clean
	@$(MAKE) -C openjpeg clean
	@$(MAKE) -C libpng16 clean
	@$(MAKE) -C zlib clean
	@$(MAKE) -C qhull clean
